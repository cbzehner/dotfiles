# Details at https://github.com/cbzehner/accessible-coreutils
alias build="make"
alias compare="diff" # or cmp or colordiff
alias copy="pbcopy"
alias count='wc'
alias create-file="touch"
alias create-folder="mkdir -p"
alias dedup='uniq'
alias delete="rm -rv"
alias docs="man"
alias duplicate="cp -RPiv"
alias edit="nvim" # or vim, emacs, code, nano, etc.
alias environment="env"
alias find-contents="rg" # or grep
alias find-name='fd' # or find or fzf
alias find="fd --type f --exclude '.git'" # or find or fzf
alias go='z' # or cd
alias help="tldr" # or man
alias jump='z' # or cd
alias list="exa --git-ignore --group-directories-first --classify" # or ls
alias move="mv -iv"
alias paste="pbpaste"
alias print="echo"
alias process='procs' # or ps
alias profile='time' # or hyperfine
alias remove="rm -rv"
alias rename="mv -iv"
alias search="rg" # or grep
alias shortcut="link -s" # Usage: <source_file> <link_location>. link and ln are the same utility
# alias source="git"
alias split='cut'
alias unique='uniq'
alias view="bat" # or cat
alias where="pwd"
